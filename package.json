{
	"name": "@nan0web/markdown",
	"version": "1.0.0",
	"description": "Markdown parser for nan0web",
	"main": "src/index.js",
	"types": "types/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"import": "./src/index.js",
			"types": "./types/index.d.ts"
		},
		"./*": "./src/*",
		"./types/*": "./types/*"
	},
	"scripts": {
		"build": "tsc",
		"clean": "rm -rf .cache/ && rm -rf dist/",
		"clean:modules": "rm -rf node_modules",
		"test": "node --test --test-timeout=9999 \"src/**/*.test.js\" | nan0test parse tap --fail",
		"test:docs": "node --test src/README.md.js",
		"test:release": "node --test \"releases/**/*.test.js\"",
		"test:coverage": "node --experimental-test-coverage --test-coverage-include=\"src/**/*.js\" --test-coverage-exclude=\"src/**/*.test.js\" --test \"src/**/*.test.js\"",
		"test:coverage:collect": "nan0test coverage",
		"test:status": "nan0test status",
		"test:all": "echo '```bash\n' > me.md && npm run test >> me.md 2>&1 && echo '```\n\n```bash\n` >> me.md && npm run build >> me.md 2>&1 && echo '```\n'",
		"release": "nan0release publish",
		"precommit": "npm test",
		"prepush": "npm test",
		"prepare": "husky",
		"playground": "node playground/main.js"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.11.0",
	"devDependencies": {
		"@nan0web/release": "^1.0.0",
		"@nan0web/types": "^1.0.0",
		"@nan0web/test": "^1.0.0",
		"husky": "^9.1.7"
	},
	"peerDependencies": {
		"@nan0web/types": "^1.0.0"
	}
}
